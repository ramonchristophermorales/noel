{
  "name": "noel-excel-app",
  "version": "1.0.0",
  "description": "Noel excel app using electron and jexcel",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . noel --all --icon=images/logo.png",
    "package-linux": "electron-packager . noel-linux --overwrite --asar=true --platform=linux --arch=x64 --icon=images/logo.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/ramonchristophermorales/noel.git",
  "keywords": [
    "Noel",
    "excel",
    "electron",
    "jexcel"
  ],
  "author": "Ramon Christopher Morales",
  "license": "MIT",
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-packager": "^8.7.0",
    "electron-reload": "^1.2.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "css-loader": "^0.28.4",
    "font-awesome": "^4.7.0",
    "jexcel": "^1.3.4",
    "jquery": "^3.2.1",
    "jquery-ui-dist": "^1.12.1",
    "mysql": "^2.15.0",
    "orm": "^4.0.2",
    "sweetalert": "^2.1.0",
    "underscore": "^1.8.3"
  }
}
